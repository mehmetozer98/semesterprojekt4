@model List<MVCEngiBeering.Models.BBMachineViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to EngiBeering</h1>
    <p>Please choose the Beer brewing machine you wish to use.</p>
    
    
    
    @foreach (var machine in Model)
    {
        <input type="Submit" value="Machine @Html.DisplayFor(modelItem => machine.id)" />
    }

</div>